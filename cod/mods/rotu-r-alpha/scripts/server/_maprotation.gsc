init()
{
	scripts\server\_maps::getMaprotation();
	level.onChangeMap = ::map_rotate;
}

map_rotate()
{
	nextmap = scripts\server\_maps::getNextMap();
	if (isdefined(nextmap))
	scripts\server\_maps::changeMap(nextmap);
	else
	{
		map_restart( false );
	}
}